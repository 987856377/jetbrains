<application>
  <component name="FileTypeManager" version="18">
    <ignoreFiles list="*.git;*.idea;*.iml;*.lck;" />
    <extensionMap>
      <mapping ext="gradle.kts" type="Kotlin" />
      <mapping ext="main.kts" type="Kotlin" />
      <mapping pattern=".tgitconfig" type="PLAIN_TEXT" />
      <mapping ext="config" type="PLAIN_TEXT" />
      <mapping ext="lck" type="PLAIN_TEXT" />
      <mapping ext="factories" type="spring.factories" />
      <removed_mapping ext="apk" approved="true" type="ARCHIVE" />
      <removed_mapping ext="psd" approved="true" type="Image" />
    </extensionMap>
  </component>
</application>